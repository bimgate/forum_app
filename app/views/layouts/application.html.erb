  <%= stylesheet_link_tag 'application', media: 'all' %>
  
  <%= csrf_meta_tags %>

  <%= render 'layouts/shim' %>
 <meta name="google-site-verification" content="9C1gwJ_2di_k2XdLCDPOMCvDUqGiQjbWAQhUCIBVMXk" /> 

</head>
<body>

<%= render 'layouts/header' %>


